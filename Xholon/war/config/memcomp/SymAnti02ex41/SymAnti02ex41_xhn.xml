<params>
	<param name="ShowParams" value="false"/>
	<param name="ModelName" value="SymAnti 2002 ex41"/>
	<param name="AppM" value="true"/>
	<param name="InfoM" value="false"/>
	<param name="ErrorM" value="true"/>
	<param name="MaxProcessLoops" value="20"/>
	<param name="SizeMessageQueue" value="20"/>
	<param name="InheritanceHierarchyFile" value="config/memcomp/SymAnti02ex41/InheritanceHierarchy.xml"/>
