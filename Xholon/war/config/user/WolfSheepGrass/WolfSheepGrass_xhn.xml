<param name="GrowGrass"           value="true"/> <!-- Grass true -->
<param name="GrassRegrowthTime"   value="30"/>   <!-- Grass 30 -->
<param name="InitialNumberSheep"  value="100"/>  <!-- Sheep 100 -->
<param name="SheepGainFromFood"   value="4.0"/>  <!-- Sheep 4.0 -->
<param name="SheepReproduce"      value="4"/>    <!-- Sheep 4 -->
<param name="InitialNumberWolves" value="50"/>   <!-- Wolf 50 -->
<param name="WolfGainFromFood"    value="20.0"/> <!-- Wolf 20.0 -->
<param name="WolfReproduce"       value="5"/>    <!-- Wolf 5 -->
