<?xml version="1.0" encoding="UTF-8"?>
<!-- TestFsm configuration file
	 Ken Webb
	 August 27, 2007 -->
<params>
	<param name="ShowParams" value="false"/>
	<param name="ModelName" value="Test of Finite State Machine"/>
	<param name="AppM" value="true"/>
	<param name="InfoM" value="false"/> <!-- true: Enable trace messages in console. false: Disable. -->
	<param name="ErrorM" value="true"/>
	<param name="MaxProcessLoops" value="10"/>
	<param name="TimeStepInterval" value="10"/>
	<param name="SizeMessageQueue" value="5"/>
	<param name="InheritanceHierarchyFile" value="config/TestFsm/InheritanceHierarchy_Fsm_KW.xml"/>
	<param name="CompositeStructureHierarchyFile" value="config/TestFsm/CompositeStructureHierarchy_Fsm_KW.xml"/>
	<param name="ClassDetailsFile" value="config/TestFsm/ClassDetails_Fsm_KW.xml"/>
	<param name="InformationFile" value="config/TestFsm/Information_Fsm_KW.xml"/>
	<param name="JavaClassName" value="org.primordion.user.app.TestFsm.AppTestFsmKW"/>
	<param name="JavaXhClassName" value="org.primordion.user.app.TestFsm.XhTestFsmKW"/>
	<param name="MaxPorts" value="1"/>
	<!-- UseDataPlotter determines whether the data each timestep is plotted on a graph. -->
	<param name="UseDataPlotter" value="google2"/> <!-- JFreeChart gnuplot google2 none -->
	<param name="DataPlotterParams"
		value="TestFsm - Active States,Time Step,Active State,./statistics/,testFsmStats,1,WRITE_AS_LONG"/>
	<!-- Whether or not to capture interactions between Xholons, and output as a graphical UML Sequence Diagram. -->
	<param name="UseInteractions" value="true"/>
	<param name="InteractionParams" value="32,true,localhost,60001"/>
	<!-- Number of the test scenario to run (1 to 5; 4 is the default). See XhTestFsm.java -->
	<param name="TestScenario" value="1"/>
</params>
