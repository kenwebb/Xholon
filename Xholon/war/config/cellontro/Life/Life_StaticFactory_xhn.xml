	<param name="UseDataPlotter" value="gnuplot"/> <!-- JFreeChart gnuplot none -->
	<param name="DataPlotterParams"
		value="Small molecule quantities changing over time,Time (timesteps),Quantities of each molecule,./statistics/,lifeStats,1,WRITE_AS_LONG"/>
	<!-- 2D Graphics -->
	<param name="UseGraphicalTreeViewer" value="false"/>
	<param name="GraphicalTreeViewerParams"
		value="./descendant::Heart/RightVentricle/BloodPlasma[1],50,50,1000,400,9,Cellontro - Life - Composite Structure,false"/>
	<param name="UseGraphicalNetworkViewer" value="false"/>
	<param name="GraphicalNetworkViewerClassName"
		value="org.primordion.cellontro.io.NetworkViewerJungCell"/>
	<param name="GraphicalNetworkViewerParams"
		value="./descendant::Heart/RightVentricle/BloodPlasma[1],1000,600,8,LAYOUT_ISOM,false,Cellontro - Life - Biochemical Network,false"/>
	<!-- VRML 3D graphics -->
	<param name="UseVrml" value="false"/>
	<!-- Interactions -->
	<param name="UseInteractions" value="false"/>
</params>
