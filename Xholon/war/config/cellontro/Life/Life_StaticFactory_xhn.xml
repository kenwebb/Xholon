<params>
	<param name="ShowParams" value="false"/>
	<param name="ModelName" value="Life - cells, organism, ecology"/>
	<param name="AppM" value="false"/>
	<param name="InfoM" value="false"/>
	<param name="ErrorM" value="true"/>
	<!-- uses TreeNodeFactoryStatic rather than TreeNodeFactoryDynamic -->
	<param name="MaxXholons" value="41000"/>
	<param name="MaxXholonClasses" value="600"/>
	<param name="MaxStateMachineEntities" value="100"/>
	<param name="MaxActivities" value="0"/>
	<param name="TreeNodeFactoryDynamic" value="false"/>
	<!-- end of StaticFactory params -->
	<param name="MaxProcessLoops" value="200"/>
	<param name="TimeStepInterval" value="300"/>
	<param name="SizeMessageQueue" value="100"/>
	<param name="InheritanceHierarchyFile" value="./config/cellontro/Cell/InheritanceHierarchy.xml"/>
	<param name="CompositeStructureHierarchyFile" value="./config/cellontro/Life/CompositeStructureHierarchy_Life7.xml"/>
	<param name="ClassDetailsFile" value="./config/cellontro/Life/ClassDetails_Life2.xml"/>
	<param name="InformationFile" value="./config/cellontro/Life/Information_life.xml"/>
	<param name="JavaClassName" value="org.primordion.cellontro.app.AppLife"/>
	<param name="JavaXhClassName" value="org.primordion.cellontro.app.XhLife"/>
	<param name="JavaXhClassClassName" value="org.primordion.cellontro.base.BioXholonClass"/>
	<param name="MaxPorts" value="8"/>
